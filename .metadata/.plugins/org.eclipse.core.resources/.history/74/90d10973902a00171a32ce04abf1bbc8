package threading;

import java.util.Calendar;

public class TestDatario extends Thread {

	public static void main(String[] args) {
		
		Datario d = new Datario();
		try {
			d.sleep(1000);
			
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
			
		}
		
		
	}
	
	

}
