
public class Prodotti extends Prodotto {
	
	private int qOrdinata;

	public Prodotti() {
		super();
	}

	public Prodotti(int codice, String prodotto, String descrizione, double prezzo, int qOrdinata) {
		super(codice, prodotto, descrizione, prezzo);
		this.qOrdinata = qOrdinata;
	}

	public int getqOrdinata() {
		return qOrdinata;
	}

	public void setqOrdinata(int qOrdinata) {
		this.qOrdinata = qOrdinata;
	}

	@Override
	public String toString() {
		return "Prodotti [" + (super.toString() != null ? "toString()=" + super.toString() + ", " : "") 
				+ "qOrdinata="+ qOrdinata + "]";
	}
	
	
	
	
	

}
