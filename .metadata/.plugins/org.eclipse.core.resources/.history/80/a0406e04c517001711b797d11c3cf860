import java.util.GregorianCalendar;


public class Test {
	
	
	public static void main(String[] args) {
		
		GregorianCalendar dataNPinco = new GregorianCalendar(12,10,1901);
		GregorianCalendar dataMPinco = new GregorianCalendar(12,10,2001);

		
		Autore a1 = new Autore ("Pinco", "Pallino"
				, dataNPinco, "Tunisi"
				, dataMPinco, "Roma");
		
		System.out.println("è vivo? "a1.isVivo());
	}

}
